{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Nome do Data Factory",
			"defaultValue": "adf-guma"
		},
		"AzureKeyVault_properties_typeProperties_baseUrl": {
			"type": "string",
			"defaultValue": "https://akv-guma.vault.azure.net/"
		},
		"ls_storagedev_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://storagedevguma.dfs.core.windows.net"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI para conta de armazenamento contendo os modelos ARM ligados"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Token sas para conta de armazenamento contendo os modelos ARM ligados"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "adf-guma_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"AzureKeyVault_properties_typeProperties_baseUrl": {
						"value": "[parameters('AzureKeyVault_properties_typeProperties_baseUrl')]"
					},
					"ls_storagedev_properties_typeProperties_url": {
						"value": "[parameters('ls_storagedev_properties_typeProperties_url')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}